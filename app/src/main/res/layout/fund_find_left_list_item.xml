<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <View
        android:layout_width="@dimen/d110"
        android:layout_height="@dimen/d48"
        android:id="@+id/FundLeftNameBg"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/trade_list_data_bg" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/FundLeftName"
        app:layout_constraintLeft_toLeftOf="@id/FundLeftNameBg"
        app:layout_constraintRight_toRightOf="@id/FundLeftNameBg"
        app:layout_constraintTop_toTopOf="@id/FundLeftNameBg"
        app:layout_constraintBottom_toTopOf="@id/FundLeftCode"
        android:textSize="@dimen/s14"
        android:maxLines="1"
        android:textColor="@color/black_333"
        android:layout_marginLeft="4dp"
        android:layout_marginRight="4dp"
        android:ellipsize="end"
        android:lines="1"/>
    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/FundLeftCode"
        app:layout_constraintLeft_toLeftOf="@id/FundLeftName"
        app:layout_constraintRight_toRightOf="@id/FundLeftName"
        app:layout_constraintBottom_toBottomOf="@id/FundLeftNameBg"
        app:layout_constraintTop_toBottomOf="@id/FundLeftName"
        android:textSize="@dimen/s12"
        android:textColor="@color/gray_999"
        android:lines="1"
        android:maxLines="1" />

    <TextView
        android:layout_width="@dimen/d110"
        android:layout_height="@dimen/d48"
        android:id="@+id/FundLeftHeavy"
        app:layout_constraintLeft_toRightOf="@id/FundLeftNameBg"
        app:layout_constraintTop_toTopOf="@id/FundLeftNameBg"
        app:layout_constraintBottom_toBottomOf="@id/FundLeftNameBg"
        android:textSize="@dimen/s14"
        android:textColor="@color/black_333"
        android:gravity="center"
        android:background="@drawable/trade_list_data_bg"/>

</androidx.constraintlayout.widget.ConstraintLayout>