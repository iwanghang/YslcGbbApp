<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <View
        android:layout_width="@dimen/d100"
        android:layout_height="@dimen/d48"
        android:id="@+id/FundOptionLeftDataBg"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/FundOptionLeftDataName"
        app:layout_constraintLeft_toLeftOf="@id/FundOptionLeftDataBg"
        app:layout_constraintRight_toRightOf="@id/FundOptionLeftDataBg"
        app:layout_constraintTop_toTopOf="@id/FundOptionLeftDataBg"
        app:layout_constraintBottom_toTopOf="@id/FundOptionLeftDataCode"
        android:maxLines="1"
        android:ellipsize="end"
        android:textSize="@dimen/s14"
        android:textColor="@color/black_333"
        android:gravity="left"/>

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/FundOptionLeftDataCode"
        app:layout_constraintLeft_toLeftOf="@id/FundOptionLeftDataName"
        app:layout_constraintRight_toRightOf="@id/FundOptionLeftDataName"
        app:layout_constraintBottom_toBottomOf="@id/FundOptionLeftDataBg"
        app:layout_constraintTop_toBottomOf="@id/FundOptionLeftDataName"
        android:textColor="@color/gray_999"
        android:textSize="@dimen/s10"
        android:gravity="left"/>

</androidx.constraintlayout.widget.ConstraintLayout>