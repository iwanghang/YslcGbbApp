<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white">

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/FundName"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toLeftOf="@id/FundCode"
        app:layout_constraintBottom_toBottomOf="parent"
        android:textColor="@color/black_333"
        android:textSize="@dimen/s14"
        android:layout_marginTop="@dimen/d14"
        android:layout_marginBottom="@dimen/d14"
        android:layout_marginLeft="@dimen/d14"
        android:gravity="left" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/FundCode"
        app:layout_constraintLeft_toRightOf="@id/FundName"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/FundName"
        app:layout_constraintBottom_toBottomOf="@id/FundName"
        android:gravity="left"
        android:textSize="@dimen/s14"
        android:textColor="@color/black_333"
        android:layout_marginRight="74dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>