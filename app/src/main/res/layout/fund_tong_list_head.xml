<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <View
        android:layout_width="@dimen/d90"
        android:layout_height="@dimen/d48"
        android:id="@+id/FundTongDayRiseBg"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/FundTongDayRise"
        app:layout_constraintLeft_toLeftOf="@id/FundTongDayRiseBg"
        app:layout_constraintRight_toRightOf="@id/FundTongDayRiseBg"
        app:layout_constraintTop_toTopOf="@id/FundTongDayRiseBg"
        android:textColor="@color/gray_666"
        android:textSize="@dimen/s14"
        android:text="日涨幅"
        android:lines="1"
        app:layout_constraintBottom_toBottomOf="@id/FundTongDayRiseBg"/>
    <ImageView
        android:layout_width="@dimen/d10"
        android:layout_height="@dimen/d10"
        android:id="@+id/FundTongDayImg"
        app:layout_constraintLeft_toRightOf="@id/FundTongDayRise"
        app:layout_constraintTop_toTopOf="@id/FundTongDayRise"
        app:layout_constraintBottom_toBottomOf="@id/FundTongDayRise"
        android:scaleType="fitCenter"
        android:adjustViewBounds="true"
        android:src="@drawable/da_xiao" />

    <View
        android:layout_width="@dimen/d90"
        android:layout_height="@dimen/d48"
        android:id="@+id/FundTongWeekRiseBg"
        app:layout_constraintLeft_toRightOf="@id/FundTongDayRiseBg"
        app:layout_constraintTop_toTopOf="@id/FundTongDayRiseBg"
        app:layout_constraintBottom_toBottomOf="@id/FundTongDayRiseBg" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/FundTongWeekRise"
        app:layout_constraintLeft_toLeftOf="@id/FundTongWeekRiseBg"
        app:layout_constraintRight_toRightOf="@id/FundTongWeekRiseBg"
        app:layout_constraintTop_toTopOf="@id/FundTongWeekRiseBg"
        android:textColor="@color/gray_666"
        android:textSize="@dimen/s14"
        android:text="周涨幅"
        android:lines="1"
        app:layout_constraintBottom_toBottomOf="@id/FundTongDayRiseBg"/>
    <ImageView
        android:layout_width="@dimen/d10"
        android:layout_height="@dimen/d10"
        android:id="@+id/FundTongWeekRiseImg"
        app:layout_constraintLeft_toRightOf="@id/FundTongWeekRise"
        app:layout_constraintTop_toTopOf="@id/FundTongWeekRise"
        app:layout_constraintBottom_toBottomOf="@id/FundTongWeekRise"
        android:scaleType="fitCenter"
        android:adjustViewBounds="true"
        android:src="@drawable/da_xiao" />

    <View
        android:layout_width="@dimen/d90"
        android:layout_height="0dp"
        android:id="@+id/FundTongMonthRiseBg"
        app:layout_constraintLeft_toRightOf="@id/FundTongWeekRiseBg"
        app:layout_constraintTop_toTopOf="@id/FundTongWeekRiseBg"
        app:layout_constraintBottom_toBottomOf="@id/FundTongWeekRiseBg" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/FundTongMonthRise"
        app:layout_constraintLeft_toLeftOf="@id/FundTongMonthRiseBg"
        app:layout_constraintRight_toRightOf="@id/FundTongMonthRiseBg"
        app:layout_constraintTop_toTopOf="@id/FundTongMonthRiseBg"
        app:layout_constraintBottom_toBottomOf="@id/FundTongMonthRiseBg"
        android:textSize="@dimen/s14"
        android:textColor="@color/gray_666"
        android:text="月涨幅" />
    <ImageView
        android:layout_width="@dimen/d10"
        android:layout_height="@dimen/d10"
        android:id="@+id/FundTongMonthRiseImg"
        app:layout_constraintLeft_toRightOf="@id/FundTongMonthRise"
        app:layout_constraintTop_toTopOf="@id/FundTongMonthRise"
        app:layout_constraintBottom_toBottomOf="@id/FundTongMonthRise"
        android:scaleType="fitCenter"
        android:adjustViewBounds="true"
        android:src="@drawable/da_xiao" />

    <View
        android:layout_width="@dimen/d90"
        android:layout_height="0dp"
        android:id="@+id/FundTongQuarterRiseBg"
        app:layout_constraintLeft_toRightOf="@id/FundTongMonthRiseBg"
        app:layout_constraintTop_toTopOf="@id/FundTongMonthRiseBg"
        app:layout_constraintBottom_toBottomOf="@id/FundTongMonthRiseBg" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/FundTongQuarterRise"
        app:layout_constraintLeft_toLeftOf="@id/FundTongQuarterRiseBg"
        app:layout_constraintRight_toRightOf="@id/FundTongQuarterRiseBg"
        app:layout_constraintTop_toTopOf="@id/FundTongQuarterRiseBg"
        app:layout_constraintBottom_toBottomOf="@id/FundTongQuarterRiseBg"
        android:textSize="@dimen/s14"
        android:textColor="@color/gray_666"
        android:text="季度涨幅" />
    <ImageView
        android:layout_width="@dimen/d10"
        android:layout_height="@dimen/d10"
        android:id="@+id/FundTongQuarterRiseImg"
        app:layout_constraintLeft_toRightOf="@id/FundTongQuarterRise"
        app:layout_constraintTop_toTopOf="@id/FundTongQuarterRise"
        app:layout_constraintBottom_toBottomOf="@id/FundTongQuarterRise"
        android:scaleType="fitCenter"
        android:adjustViewBounds="true"
        android:src="@drawable/da_xiao" />

    <View
        android:layout_width="@dimen/d90"
        android:layout_height="0dp"
        android:id="@+id/FundTongHalfYearRiseBg"
        app:layout_constraintLeft_toRightOf="@id/FundTongQuarterRiseBg"
        app:layout_constraintTop_toTopOf="@id/FundTongQuarterRiseBg"
        app:layout_constraintBottom_toBottomOf="@id/FundTongQuarterRiseBg" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/FundTongHalfYearRise"
        app:layout_constraintLeft_toLeftOf="@id/FundTongHalfYearRiseBg"
        app:layout_constraintRight_toRightOf="@id/FundTongHalfYearRiseBg"
        app:layout_constraintTop_toTopOf="@id/FundTongHalfYearRiseBg"
        app:layout_constraintBottom_toBottomOf="@id/FundTongHalfYearRiseBg"
        android:textSize="@dimen/s14"
        android:textColor="@color/gray_666"
        android:text="半年涨幅" />
    <ImageView
        android:layout_width="@dimen/d10"
        android:layout_height="@dimen/d10"
        android:id="@+id/FundTongHalfYearRiseImg"
        app:layout_constraintLeft_toRightOf="@id/FundTongHalfYearRise"
        app:layout_constraintTop_toTopOf="@id/FundTongHalfYearRise"
        app:layout_constraintBottom_toBottomOf="@id/FundTongHalfYearRise"
        android:scaleType="fitCenter"
        android:adjustViewBounds="true"
        android:src="@drawable/da_xiao" />

    <View
        android:layout_width="@dimen/d90"
        android:layout_height="0dp"
        android:id="@+id/FundTongYearRiseBg"
        app:layout_constraintLeft_toRightOf="@id/FundTongHalfYearRiseBg"
        app:layout_constraintTop_toTopOf="@id/FundTongHalfYearRiseBg"
        app:layout_constraintBottom_toBottomOf="@id/FundTongHalfYearRiseBg" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/FundTongYearRise"
        app:layout_constraintLeft_toLeftOf="@id/FundTongYearRiseBg"
        app:layout_constraintRight_toRightOf="@id/FundTongYearRiseBg"
        app:layout_constraintTop_toTopOf="@id/FundTongYearRiseBg"
        app:layout_constraintBottom_toBottomOf="@id/FundTongYearRiseBg"
        android:textSize="@dimen/s14"
        android:textColor="@color/gray_666"
        android:text="年涨幅" />
    <ImageView
        android:layout_width="@dimen/d10"
        android:layout_height="@dimen/d10"
        android:id="@+id/FundTongYearRiseImg"
        app:layout_constraintLeft_toRightOf="@id/FundTongYearRise"
        app:layout_constraintTop_toTopOf="@id/FundTongYearRise"
        app:layout_constraintBottom_toBottomOf="@id/FundTongYearRise"
        android:scaleType="fitCenter"
        android:adjustViewBounds="true"
        android:src="@drawable/da_xiao" />

    <View
        android:layout_width="@dimen/d90"
        android:layout_height="0dp"
        android:id="@+id/FundTongFValueBg"
        app:layout_constraintLeft_toRightOf="@id/FundTongYearRiseBg"
        app:layout_constraintTop_toTopOf="@id/FundTongYearRiseBg"
        app:layout_constraintBottom_toBottomOf="@id/FundTongYearRiseBg" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/FundTongFValue"
        app:layout_constraintLeft_toLeftOf="@id/FundTongFValueBg"
        app:layout_constraintRight_toRightOf="@id/FundTongFValueBg"
        app:layout_constraintTop_toTopOf="@id/FundTongFValueBg"
        app:layout_constraintBottom_toBottomOf="@id/FundTongFValueBg"
        android:textSize="@dimen/s14"
        android:textColor="@color/gray_666"
        android:text="基金规模" />
    <ImageView
        android:layout_width="@dimen/d10"
        android:layout_height="@dimen/d10"
        android:id="@+id/FundTongFValueImg"
        app:layout_constraintLeft_toRightOf="@id/FundTongFValue"
        app:layout_constraintTop_toTopOf="@id/FundTongFValue"
        app:layout_constraintBottom_toBottomOf="@id/FundTongFValue"
        android:scaleType="fitCenter"
        android:adjustViewBounds="true"
        android:src="@drawable/da_xiao" />

