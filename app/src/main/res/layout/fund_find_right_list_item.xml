<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/FundRightEva"
        android:layout_width="@dimen/d110"
        android:layout_height="@dimen/d48"
        android:ellipsize="end"
        android:gravity="center"
        android:maxLines="1"
        android:textColor="@color/gray_666"
        android:textSize="@dimen/s14"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="@drawable/trade_list_data_bg" />

    <TextView
        android:id="@+id/FundRightNetWorth"
        android:layout_width="@dimen/d110"
        android:layout_height="@dimen/d48"
        android:ellipsize="end"
        android:gravity="center"
        android:maxLines="1"
        android:textColor="@color/gray_666"
        android:textSize="@dimen/s14"
        app:layout_constraintLeft_toRightOf="@id/FundRightEva"
        app:layout_constraintTop_toTopOf="@id/FundRightEva"
        app:layout_constraintBottom_toBottomOf="@id/FundRightEva"
        android:background="@drawable/trade_list_data_bg" />

    <TextView
        android:id="@+id/FundRightMonthGains"
        android:layout_width="@dimen/d110"
        android:layout_height="@dimen/d48"
        android:ellipsize="end"
        android:gravity="center"
        android:maxLines="1"
        android:textColor="@color/gray_666"
        android:textSize="@dimen/s14"
        app:layout_constraintLeft_toRightOf="@id/FundRightNetWorth"
        app:layout_constraintTop_toTopOf="@id/FundRightEva"
        app:layout_constraintBottom_toBottomOf="@id/FundRightEva"
        android:background="@drawable/trade_list_data_bg" />

    <TextView
        android:id="@+id/FundRightQuarterGains"
        android:layout_width="@dimen/d110"
        android:layout_height="@dimen/d48"
        android:ellipsize="end"
        android:gravity="center"
        android:maxLines="1"
        android:textColor="@color/gray_666"
        android:textSize="@dimen/s14"
        app:layout_constraintLeft_toRightOf="@id/FundRightMonthGains"
        app:layout_constraintTop_toTopOf="@id/FundRightEva"
        app:layout_constraintBottom_toBottomOf="@id/FundRightEva"
        android:background="@drawable/trade_list_data_bg" />

    <TextView
        android:id="@+id/FundRightHalfYearGains"
        android:layout_width="@dimen/d110"
        android:layout_height="@dimen/d48"
        android:ellipsize="end"
        android:gravity="center"
        android:maxLines="1"
        android:textColor="@color/gray_666"
        android:textSize="@dimen/s14"
        app:layout_constraintLeft_toRightOf="@id/FundRightQuarterGains"
        app:layout_constraintTop_toTopOf="@id/FundRightEva"
        app:layout_constraintBottom_toBottomOf="@id/FundRightEva"
        android:background="@drawable/trade_list_data_bg" />

    <TextView
        android:id="@+id/FundRightYearGains"
        android:layout_width="@dimen/d110"
        android:layout_height="@dimen/d48"
        android:ellipsize="end"
        android:gravity="center"
        android:maxLines="1"
        android:textColor="@color/gray_666"
        android:textSize="@dimen/s14"
        app:layout_constraintLeft_toRightOf="@id/FundRightHalfYearGains"
        app:layout_constraintTop_toTopOf="@id/FundRightEva"
        app:layout_constraintBottom_toBottomOf="@id/FundRightEva"
        android:background="@drawable/trade_list_data_bg" />

</androidx.constraintlayout.widget.ConstraintLayout>